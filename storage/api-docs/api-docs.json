{
    "openapi": "3.0.0",
    "info": {
        "title": "Swagger Laravel",
        "description": "This is a sample Laravel server.",
        "contact": {
            "email": "apiteam@swagger.io"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000",
            "description": "SwaggerHUB API Mocking"
        }
    ],
    "paths": {
        "/api/articles": {
            "post": {
                "tags": [
                    "article"
                ],
                "summary": "Add a new article to the store",
                "operationId": "addArticle",
                "requestBody": {
                    "$ref": "#/components/requestBodies/Article"
                },
                "responses": {
                    "405": {
                        "description": "Invalid input"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/articles/{id}": {
            "get": {
                "tags": [
                    "article"
                ],
                "summary": "Find article by ID",
                "description": "Returns a single article",
                "operationId": "getArticleById",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of article to return",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Article"
                                }
                            },
                            "application/xml": {
                                "schema": {
                                    "$ref": "#/components/schemas/Article"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid ID supplier"
                    },
                    "404": {
                        "description": "Article not found"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            },
            "put": {
                "tags": [
                    "article"
                ],
                "summary": "Update an existing article",
                "operationId": "updateArticle",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "id of article that to be updated",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "$ref": "#/components/requestBodies/Article"
                },
                "responses": {
                    "400": {
                        "description": "Invalid ID article"
                    },
                    "404": {
                        "description": "Article not found"
                    },
                    "405": {
                        "description": "Validation exception"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            },
            "delete": {
                "tags": [
                    "article"
                ],
                "summary": "Deletes a article",
                "operationId": "deleteArticle",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Article id to delete",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "400": {
                        "description": "Invalid ID supplied"
                    },
                    "404": {
                        "description": "Article not found"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        }
    },
    "components": {
        "schemas": {
            "ApiResponse": {
                "title": "Api response",
                "description": "Api response",
                "properties": {
                    "code": {
                        "title": "Code",
                        "description": "Code",
                        "type": "integer",
                        "format": "int32"
                    },
                    "message": {
                        "title": "Message",
                        "description": "Message",
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "Article": {
                "title": "Article model",
                "description": "Article model",
                "required": [
                    "title",
                    "body"
                ],
                "properties": {
                    "title": {
                        "title": "Article name",
                        "description": "Article name",
                        "type": "string"
                    },
                    "body": {
                        "title": "Article body",
                        "description": "Article body",
                        "type": "string"
                    },
                    "photoUrls": {
                        "title": "Photo urls",
                        "description": "Photo urls",
                        "type": "string"
                    }
                },
                "type": "object"
            }
        },
        "requestBodies": {
            "Article": {
                "description": "Article object that needs to be added to the store",
                "required": true,
                "content": {
                    "application/xml": {
                        "schema": {
                            "$ref": "#/components/schemas/Article"
                        }
                    },
                    "application/json": {
                        "schema": {
                            "$ref": "#/components/schemas/Article"
                        }
                    }
                }
            }
        }
    },
    "tags": [
        {
            "name": "article",
            "description": "Everything about your Article"
        }
    ],
    "externalDocs": {
        "description": "Find out more about Swagger",
        "url": "http://swagger.io"
    }
}